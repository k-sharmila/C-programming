#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n,r,a[30],i=0,sum=0;
    scanf("%d", &n);
    while(n!=0)
    {
        r=n%10;
        a[i]=r;
        sum=sum+a[i];
        i++;
        n=n/10;
    }
    
        printf("%d",sum);
    
    //Complete the code to calculate the sum of the five digits on n.
    return 0;
}
